{
    "presets": ["next/babel"],
    "env": {
        "production": {
            "plugins": [["inline-dotenv",{
                "path": ".env.production"
            }]]
        },
        "development": {
            "plugins": [["inline-dotenv",{
                "path": ".env.local"
            }]]
        },
        "test": {
            "plugins": [["inline-dotenv",{
                "path": ".env.test"
            }], ["istanbul", {
                "include": ["/pages/**/*", "components/**/*"]
            }]]
        }
    }
}